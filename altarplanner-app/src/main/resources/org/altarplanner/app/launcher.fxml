<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="200.0" maxHeight="-Infinity" maxWidth="-Infinity"
      alignment="CENTER" spacing="10.0"
      xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.altarplanner.app.Launcher">
    <Label text="%launcher.label.altarplanner">
        <font>
            <Font size="24.0"/>
        </font>
    </Label>
    <Separator/>
    <Label text="%launcher.label.generalConfiguration" underline="true">
        <font>
            <Font size="16"/>
        </font>
    </Label>
    <Button mnemonicParsing="false" text="%launcher.button.editServiceTypes" onAction="#loadServiceTypeEditor"/>
    <Button mnemonicParsing="false" text="%launcher.button.editRegularMasses" onAction="#loadRegularMassEditor"/>
    <Button mnemonicParsing="false" text="%launcher.button.editServers" onAction="#loadServerEditor"/>
    <Separator/>
    <Label text="%launcher.label.scheduleCreation" underline="true">
        <font>
            <Font size="16"/>
        </font>
    </Label>
    <Button mnemonicParsing="false" text="%launcher.button.createDiscreteMasses" onAction="#loadDiscreteMassEditor"/>
    <Button mnemonicParsing="false" text="%launcher.button.planServices" onAction="#loadSolverView"/>
    <Button mnemonicParsing="false" text="%launcher.button.exportSchedule" onAction="#exportSchedule"/>
</VBox>
